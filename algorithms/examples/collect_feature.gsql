CREATE QUERY collect_feature(VERTEX source) FOR GRAPH movie returns (MapAccum<STRING, FLOAT>){
  MapAccum<STRING, FLOAT> @@feature_vector;
  Source = {source};
  Source = SELECT s
           FROM Source:s -(Likes:e) -> :t
           ACCUM @@feature_vector += (t.name -> e.weight);
  RETURN @@feature_vector;
  }
